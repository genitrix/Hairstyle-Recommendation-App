.store-list{
  padding: 60rpx 32rpx 40rpx;
  position: relative;
}

.store-title{
  
  display: flex;
  align-items: center;
  justify-content: space-between;
}

.store-title text {
  font-size: 36rpx;
}

.price{
  padding-top: 20rpx;
  position: relative;
}

.red-tip{
  position: absolute;
  right: 0;
  color:#ff3333;
}

.price-text{
  padding-left: 13rpx;
}

.activity{
  padding-top: 35rpx;
}

.activity-content{
  font-size: 22rpx;
  color:#898989;
  padding-left: 10rpx;
}

.button{
  margin-top: 33rpx;
}

.adress {
  padding-top: 24rpx;
}

.adress-text{
  padding-left: 15rpx;
  color:#666;
  font-size: 22rpx;
}

.position-button{
  position: absolute;
  right: 32rpx;
  top:230rpx;
}